div.timeline
	.horizTimeLine("*ngIf"='patient')
		.div(style="width:100%; height:12px; background-color:#EEEAEA;")
			.div("*ngFor"='let t of timeline', (mouseenter)='popup.open(t)', (mouseleave)='popup.close()', style="float:right; width:22px; height:22px; border: 1px solid black; background-color:white; border-radius:5px; top:20px; margin-right:7%")
	.displayBox(id='content', hidden=true)

timeline-popup(#popup='')